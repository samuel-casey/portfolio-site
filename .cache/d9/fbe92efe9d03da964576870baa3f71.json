n\tcolor: inherit;\n}\n\nmain {\n\tdisplay: grid;\n\tgrid-template-areas:\n\t\t'about'\n\t\t'projects'\n\t\t'writing'\n\t\t'contact';\n\tmargin: 5%;\n\tjustify-content: center;\n\tword-wrap: break-word;\n\n\tspan.glyphicon.glyphicon-chevron-down {\n\t\tcolor: blue;\n\t}\n\n\ti#scrollArrow {\n\t\tposition: fixed;\n\t\tbottom: 5px;\n\t\tright: 1px;\n\t\tcolor: $sudbury-green;\n\t\tfont-size: 0.8rem;\n\t}\n\n\thr.gray {\n\t\talign-self: center;\n\t\twidth: 90vw;\n\t\tborder-top: 1px dashed $not-white;\n\t\tmargin: 1rem auto;\n\t}\n\n\thr.green {\n\t\talign-self: center;\n\t\twidth: 70vw;\n\t\tborder-top: 1px dashed $sudbury-green-btn-shadow;\n\t\tmargin: 1rem auto;\n\t}\n\n\tarticle#aboutContainer {\n\t\tgrid-area: about;\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t\tline-height: 1.25rem;\n\t\twidth: 99%;\n\n\t\tdiv#technologies {\n\t\t\tdisplay: grid;\n\t\t\tgrid-template-areas:\n\t\t\t\t'header header header header'\n\t\t\t\t'top top top top'\n\t\t\t\t'middle middle middle middle'\n\t\t\t\t'bottom bottom bottom bottom';\n\t\t\talign-self: center;\n\t\t\twidth: fit-content;\n\t\t\tgrid-gap: 0.1rem;\n\t\t\t// margin-left: -2%;\n\n\t\t\th3 {\n\t\t\t\tfont-size: 1.25rem;\n\t\t\t\tgrid-area: header;\n\t\t\t\tjustify-self: center;\n\t\t\t\tfont-style: normal;\n\t\t\t\tmargin-bottom: 1rem;\n\t\t\t}\n\n\t\t\timg {\n\t\t\t\twidth: 1.3rem;\n\t\t\t}\n\n\t\t\tsvg {\n\t\t\t\twidth: 1.3rem;\n\t\t\t\theight: 1.3rem;\n\t\t\t}\n\n\t\t\ti {\n\t\t\t\tfont-size: 1.25rem;\n\t\t\t}\n\n\t\t\tdiv.techLogo.js {\n\t\t\t\tgrid-area: bottom;\n\t\t\t\tgrid-column: 3;\n\t\t\t}\n\n\t\t\tdiv.techLogo.sass {\n\t\t\t\tgrid-area: bottom;\n\t\t\t\tgrid-column: 2;\n\t\t\t}\n\n\t\t\tdiv {\n\t\t\t\tdisplay: flex;\n\t\t\t\tflex-direction: column;\n\t\t\t\talign-items: center;\n\t\t\t\tjustify-content: center;\n\n\t\t\t\tp {\n\t\t\t\t\tcolor: $sudbury-green;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\n\t\tdiv.headshotContainer {\n\t\t\tdisplay: flex;\n\t\t\talign-self: center;\n\t\t\tmargin-bottom: 0.5rem;\n\t\t\